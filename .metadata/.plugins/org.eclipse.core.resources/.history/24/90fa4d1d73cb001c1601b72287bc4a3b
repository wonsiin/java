package stream;

class ThreadA1 extends Thread{//is -a 도 Thread가 된다.

	@Override
	public void run() {
		for(int i =0; i < 150; ++i) {
			System.out.println("ThreadA 실행중 =>" + i);
			if(i % 10 == 0) System.out.println();
		}
	}	
}

class ThreadB1 implements Runnable{
	@Override
	public void run() {
		int cnt = 0;
		for(char ch = 'A'; ch <= 'z'; ++ch) {
			System.out.print(ch + " ");
			if(ch % 10 == 0) System.out.println();
			if(cnt >= 10) break;
			if(ch == 'z') {
				ch = 'A';
				cnt++;
				continue;
			}
		}
	}
	
}

public class ThreadTest1 {

	public static void main(String[] args) {
		ThreadA1 th1 = new ThreadA1();					//Thread Class 시 
		Thread th2 = new Thread(new ThreadB1());		//Thread implements 시
		th1.start();
		th2.run();
	}

}
