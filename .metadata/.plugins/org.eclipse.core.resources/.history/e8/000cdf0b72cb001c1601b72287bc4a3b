package stream;

class ThreadA1 extends Thread{//is -a 도 Thread가 된다.

	@Override
	public void run() {
		for(int i =0; i < 10000000; ++i) {
			System.out.println("ThreadA 실행중 =>" + i);
		}
	}	
	
	
}

public class ThreadTest1 {

	public static void main(String[] args) {
		ThreadA1 th1 = new ThreadA1();
		th1.run();

	}

}
